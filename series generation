#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
      int n,i=1,r1=7,r2=5;
    scanf("%d",&n);
    int p=n;
    n=n/2;
    while(i<=n)
    {
      
        printf("%d ",r1);
        r1++;
        printf("%d ",r2);
        r2++;
        i++;
    }
        if(p%2!=0)
        {
            printf("%d",r1);
        }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
