#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     int t;
    scanf("%d",&t);
    if(t<100||t>200)
    {
       printf("INVALID INPUT");
    }
    else
    {
        int s=t/4;
        int rem=t%4;
        int last=rem+s;
      printf("%d\n%d\n%d\n%d",s,s,s,last);
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
