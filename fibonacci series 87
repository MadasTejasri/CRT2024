#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
       int n,i=3,r1=1,r2=1,r3=0;
    scanf("%d",&n);
    while(i<=n)
    {
       r3=r1+r2;
       r1=r2;
        r2=r3;
        i++;
    }
    printf("%d",r2);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
