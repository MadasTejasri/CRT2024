#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
 int n;
    scanf("%d",&n);
    int a[n],i;
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    int large=a[0];
    for(i=0;i<n;i++)
    {
        if(large<a[i])
         large=a[i];
    }
    printf("%d",large);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
