import java.util.*;
class DivisionWithout/
{
    public static void main(String args[])
    {
        Scanner s=new Scanner(System.in);
        int m=s.nextInt();
        int n=s.nextInt();
        int c=0;
        while(m-n>=0)
        {
            c++;
            m=m-n;
        }
        System.out.println(c);
    }
}
