#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,n,s=0;
    scanf("%d %d",&a,&n);
    while(a<=n)
    {
        s=s+a;
        a=a+1;
    }
    printf("%d",s);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
