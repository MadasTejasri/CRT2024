#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
   int n,i=1,c=0;
    scanf("%d",&n);
    while(n>=2)
    {
       i=i*n;
        n=n-1;
    }
    int r=i%10;
    while(r==0)
    {
        c=c+1;
        i=i/10;
        r=i%10;
    }
    printf("%d",c);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
