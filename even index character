#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     char str[100];
    scanf("%s",str);
    int i;
    for(i=0;str[i]!='\0';i++)
    {
        if(i%2==0)
        {
            printf("%d %c\n",i,str[i]);
        }
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
