#include <stdio.h>
#include<string.h>
int main() {
    // Write C code here
    char str1[50],str2[50],str3[100];
    int i,j;
    scanf("%s",str1);
    scanf("%s",str2);
    for(i=0;str1[i]!='\0';i++)
    {
        str3[i]=str1[i];
    }
    for(j=0;str2[j]!='\0';j++)
    {
        str3[i+j]=str2[j];
    }
    str3[i+j]='\0';
   
    printf("%s",str3);
    return 0;
}
