#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
      int m,n;
    scanf("%d%d",&m,&n);
    int a[m][n],b[m][n],i,j;
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
           scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
           scanf("%d",&b[i][j]);
        }
    }
    int sum;
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            sum=a[i][j]+b[i][j];
           printf("%d ",sum);
        }
        printf("\n");
    }
    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
