#include <stdio.h>
#include<string.h>
int main() {
    // Write C code here
    char str[100];
    int i;
    scanf("%s",str);
    for(i=0;str[i]!='\0';i++)
    {
        if(str[i]>=65&&str[i]<=90)
            str[i]=str[i]+32;
        else
           str[i]=str[i]-32;
    }
      printf("%s",str);
    return 0;
}
OUTPUT:
ProgRammIng
pROGrAMMiNG
