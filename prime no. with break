#include <stdio.h>

int main() {
   int i,n,c=0;
   scanf("%d",&n);
   for(i=2;i<n;i++)
   {
       if(n%i==0)
       {
           c++;
           break;
       }
   }
       if(c==0)
       {
           printf("Prime Number");
       }
       else
       {
           printf("Not a prime Number");
       }
    return 0;
}
