#include <stdio.h>
#include<string.h>
int main() {
    // Write C code here
    char str1[50],str2[50];
    int i,n,j=0;
    scanf("%s",str1);
    for(n=0;str1[n]!='\0';n++);
    for(i=n-1;i>=0;i--)
    {
        str2[j]=str1[i];
        j++;
    }
    str2[j]='\0';
    printf("%s\n%s",str1,str2);
    return 0;
}
