include <stdio.h>

int main() {
    // Write C code here
    int n;
    scanf("%d",&n);
    int a[n],i;
    for(i=0;i<n;i++)
    {
     scanf(" %d",&a[i]);
    }
    int large=a[0];
    
    for(i=1;i<n;i++)
    {
        if(large<a[i])
            large=a[i];
    }
    printf("%d",large);
    return 0;
}
