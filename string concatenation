#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char str1[50],str2[50],str3[100];
    scanf("%s%s",str1,str2);
    int i,j;
    for(i=0;str1[i]!='\0';i++)
    {
        str3[i]=str1[i];
    }
    str3[i]=' ';
    for(j=0;str2[j]!='\0';j++)
    {
         str3[i+j+1]=str2[j];
    }
    str3[i+j+1]='\0';
    printf("%s",str3);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
