#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
 int n;
    scanf("%d",&n);
    int a[n],i,s1=0,s2=0;
    for(i=0;i<n;i++)
    {
      scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
       if(a[i]%2==0)
       {
           s1=s1+a[i];
       }
       if(a[i]%2!=0)
       {
           s2=s2+a[i];
       }
    }
    printf("%d\n%d",s2,s1);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
