class Solution {
    static int fib(int a[],int n)
    {
         a[0]=0;
         a[1]=1;
         for(int i=2;i<=n;i++)
         {
             a[i]=(a[i-1]+a[i-2])%1000000007;
         }
         return a[n];
    }
    static int nthFibonacci(int n){
        int a[]=new int[n+1];
        Arrays.fill(a,-1);
        return fib(a,n);
    }
}
