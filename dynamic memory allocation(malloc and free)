#include <stdio.h>
#include<stdlib.h>
#include<string.h>
int main() {
    int *a,*b,*c;
    a=(int*)malloc(sizeof(int));
    b=(int*)malloc(sizeof(int));
    scanf("%d%d",a,b);
    c=(int*)malloc(sizeof(int));
    *c=*a+*b;
    free(a);
    free(b);
    printf("sum=%d",*c);
    free(c);
    // Write C code here

    return 0;
}
