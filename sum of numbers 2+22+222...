#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,s=0;
    scanf("%d",&n);
    while(n!=-1)
    {
      s=s+n;
      scanf("%d",&n);
    }
    printf("%d",s);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
