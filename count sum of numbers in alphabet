#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
       char str[100];
    scanf("%s",str);
    int i,s=0;
    for(i=0;str[i]!='\0';i++)
    {
        if(str[i]>=48&&str[i]<=57)
            s=s+str[i]-48;
    }
    printf("%d",s);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
