#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int r=n%10;
    while(n>0)
    {
         int s=n%10;
        if(s<r)
        {
        r=s;
        }
        n=n/10;
    }
    printf("%d",r);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
