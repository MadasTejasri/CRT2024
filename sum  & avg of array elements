#include <stdio.h>

int main() {
    // Write C code here
    int n;
    scanf("%d",&n);
    int a[n],i;
    for(i=0;i<n;i++)
    {
     scanf(" %d",&a[i]);
    }
    int sum=0;
    for(i=0;i<n;i++)
    {
        sum=sum+a[i];
    }
    float avg=(float)sum/n;
    printf("%d\n%.2f",sum,avg);
    return 0;
}
