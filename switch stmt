// Online C compiler to run C program online
#include <stdio.h>
int main() {
    // Write C code here
     int a,b;
     scanf("%d %d",&a,&b);
     int ch;
     scanf("%d",&ch);
     switch(ch)
     {
         case 1:
         printf("add is %d",a+b);
         break;
         case 2:
         printf("subtract is %d ",a-b);
         break;
         case 3:
         printf("multiply is %d",a*b);
         break;
         case 4:
         printf("division is %d",a/b);
         break;
         default:
         printf("invalid");
    }
    return 0;
}
